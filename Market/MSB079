<script type="text/javascript">
var IE = document.all?true:false;

function close_info()
{
	$('#mcpcp_addprof_info').hide();
}

function post_addport()
{
	var im_id = document.addport_frm.im_id.value;
	var im_fundclass = document.addport_frm.im_fundclass.value;
	var scheme_name = document.addport_frm.scheme_name.value;

	var buy_dt = "";
	var buy_qty = "";
	var buy_price = "";
	document.addport_frm.referer.value = "http://www.moneycontrol.com/portfolio_plus/add_mf_pc.php?postvalues="+im_id+"|"+im_fundclass+"|"+scheme_name+"|"+buy_dt+"|"+buy_qty+"|"+buy_price;
	document.addport_frm.postvalues.value = im_id+"|"+im_fundclass+"|"+scheme_name+"|"+buy_dt+"|"+buy_qty+"|"+buy_price;
	document.addport_frm.submit();
	return true;
}

function post_addwl()
{
	var im_id = document.addport_frm.im_id.value;
	var im_fundclass = document.addport_frm.im_fundclass.value;
	var scheme_name = document.addport_frm.scheme_name.value;
	var postvalues = im_id+"~"+im_fundclass+"~"+scheme_name;
	document.addwl_frm.referer.value = "http://www.moneycontrol.com/portfolio_plus/add_mf_wl_pc.php?postvalues="+postvalues;
	document.addwl_frm.submit();
	return true;
}

function get_plan_options()
{
	var p_imid = document.planname_frm.p_imid.value;
	var p_ptype = document.planname_frm.sel_plan_type.value;
	if(p_ptype == "")
	{
		alert("Please select a Plan Type!!!");
		document.planname_frm.sel_plan_type.focus();
		return false;
	}
	else
	{
		if(p_imid != "" && p_ptype != "")
		{
			var datafile = "/mf/mf_info/get_planoptions.php?sch_id="+p_imid+"&plan_type="+p_ptype;
			$.ajax({url:datafile, success:function(result)
			{
				if(IE)
				{
					var disp_str = "<select name='sel_plan_option' id='sel_plan_option'>"+result+"</select>";
					document.planname_frm.sel_plan_option.outerHTML = disp_str;
				}
				else
				{
					var disp_str = result;
					document.planname_frm.sel_plan_option.innerHTML = disp_str;
				}
			}});
		}
	}
}

function redirect_scheme(scheme_det)
{
	var sch_val = scheme_det;		//document.planname_frm.sel_plan_option.value;
	if(sch_val == "")
	{
		alert("Pleae select a Plan Option!!!");
		document.planname_frm.sel_plan_option.focus();
		return false;
	}
	else
	{
		var sch_det = sch_val.split("|");
		window.location.href = "/mutual-funds/nav/"+sch_det[1]+"/"+sch_det[0];
		//return false;
	}
}

function addsip()
{
	var im_id = document.addsip_frm.im_id.value;
	var im_fundclass = document.addsip_frm.im_fundclass.value;
	var scheme_name = document.addsip_frm.scheme_name.value;
	var postvalues = im_id+"~"+im_fundclass+"~"+scheme_name;

	document.addsip_frm.referer.value = "http://www.moneycontrol.com/portfolio_plus/add_mf_sip_pc.php?postvalues="+postvalues;
	document.addsip_frm.submit();
	return true;
}
</script>

