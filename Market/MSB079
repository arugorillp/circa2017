				<script>
				if(readCookie('cltop'))
				{
					//$('#tpnav_ddl1').css('display','none');
					//$('#gb_tpnav_sec1').removeClass('act');
				}
				else
				{
					//$('#tpnav_ddl1').css('display','block');
				}
				if(readCookie('nnmc'))
				{
					var usernnmc = readCookie('nnmc');					
					usernnmc = usernnmc.replace("%40","@");
					usernnmc = usernnmc.replace("+"," ");
					var dispnm =usernnmc;

					if(usernnmc.length > 12)
						dispnm = usernnmc.substr(0,11)+'..';
					else
						dispnm = usernnmc;
					
					document.getElementById('usr_nm').innerHTML ='<span class="user_icon"></span>'+dispnm;	 

					$('#blogin').hide();
					$('#alogin').show();
					if(usernnmc !='guest')
					{
						$('#usr_nm').hover(function(){
							$('#tpnav_ddl6').show();
							$(this).addClass('selcact');
							$('#tpnav_ddl6').hover(function(){ return false;
							},function(){$('#tpnav_ddl6').hide();
							$('#usr_nm').removeClass('selcact');
							});
						});
					}
					
				}
